<view class="container">
    <view class="surplusTime">
        <text>{{clock}}</text>
        <view>剩余时间</view>
    </view>
    <view class="orderInfo">
        <view class="info_icon">
            <image src="../../images/mam1.png" mode="widthFix"></image>
            <image src="../../images/order1.png" mode="widthFix"></image>
            <text>man奢微spa—{{mode}}</text>
        </view>
        <view class="info_line"></view>
        <view class="info_detail">
            <text>消费地点：{{address}}</text>
            <text>支付时间：{{time_pay}}</text>
            <text>支付金额：{{total_pay}}元</text>
            <text>优惠金额：{{discount}}元</text>
        </view>
    </view>
</view>